import java.util.Scanner;

public class XuLy {

	public XuLy() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		DanhSachChuyenXe objDSCX = new DanhSachChuyenXe();
		Scanner scan =new Scanner (System.in);
		objDSCX.nhap(scan);
		objDSCX.xuat();
		objDSCX.tinhTongDoanhThu();
		System.out.println("Tổng doanh thu:"+ objDSCX.getTongDoanhThu());
		objDSCX.tinhDoanhThuNoi();
		System.out.println("Tổng doanh thu nội:"+ objDSCX.getDoanhThuNoi());
		objDSCX.tinhDoanhThuNgoai();
		System.out.println("Tổng doanh thu ngoại:"+ objDSCX.getDoanhThuNgoai());
	}

}
